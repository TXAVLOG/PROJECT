# FILE BY TXA
# Build environment configuration
# Copy this to .env and update with your actual values

# Keystore configuration (có thể để trống nếu chỉ build debug/keystore đã được cấu hình nơi khác)
KEYSTORE_PASSWORD=
KEY_PASSWORD=

# Git configuration for commits
GIT_EMAIL=
GIT_NAME=

# Build configuration
BUILD_TYPE=release  # debug or release
CLEAN_BUILD=true    # true or false

# Repository upload options
UPLOAD_TO_REPO=false    # true or false (commits APKs to git - not recommended)
UPLOAD_TO_GITHUB=true   # true or false (uploads to GitHub Releases - recommended)
